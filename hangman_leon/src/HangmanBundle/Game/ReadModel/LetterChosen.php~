<?php

namespace HangmanBundle\Game\ReadModel;

/**
 * LetterChosen
 */
class LetterChosen
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $letter;

    /**
     * @var boolean;
     */
    private $correctGuessed;

    /**
     * @var boolean;
     */
    private $misGuessed;

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set letter
     *
     * @param string $letter
     *
     * @return LetterChosen
     */
    public function setLetter($letter)
    {
        $this->letter = $letter;

        return $this;
    }

    /**
     * Get letter
     *
     * @return string
     */
    public function getLetter()
    {
        return $this->letter;
    }

    /**
     * Set correctGuessed
     *
     * @param boolean; $correctGuessed
     *
     * @return LetterChosen
     */
    public function setCorrectGuessed(boolean $correctGuessed)
    {
        $this->correctGuessed = $correctGuessed;

        return $this;
    }

    /**
     * Get correctGuessed
     *
     * @return \boolean;
     */
    public function getCorrectGuessed()
    {
        return $this->correctGuessed;
    }

    /**
     * Set misGuessed
     *
     * @param boolean; $misGuessed
     *
     * @return LetterChosen
     */
    public function setMisGuessed(boolean $misGuessed)
    {
        $this->misGuessed = $misGuessed;

        return $this;
    }

    /**
     * Get misGuessed
     *
     * @return \boolean;
     */
    public function getMisGuessed()
    {
        return $this->misGuessed;
    }
    /**
     * @var string
     */
    private $gameId;


    /**
     * Set gameId
     *
     * @param string $gameId
     *
     * @return LetterChosen
     */
    public function setGameId($gameId)
    {
        $this->gameId = $gameId;

        return $this;
    }

    /**
     * Get gameId
     *
     * @return string
     */
    public function getGameId()
    {
        return $this->gameId;
    }
}
